  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamsellek/deck-of-cards-ts/issues"
  },
  "homepage": "https://github.com/iamsellek/deck-of-cards-ts#readme",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.0.4",
    "make-fixture": "^1.2.8",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  }
}
