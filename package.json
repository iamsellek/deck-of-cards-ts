{
  "name": "deck-of-cards-ts",
  "version": "0.0.11",
  "description": "Deck of cards package with TypeScript types.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
